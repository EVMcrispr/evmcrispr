{
  "name": "@evmcrispr/module-aragonos",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "description": "AragonOS module for EVMcrispr",
  "author": "Blossom Labs",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./utils": {
      "bun": "./src/utils/index.ts",
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    },
    "./utils/*": {
      "bun": "./src/utils/*.ts",
      "types": "./src/utils/*.ts",
      "import": "./src/utils/*.ts"
    },
    "./types": {
      "bun": "./src/types/index.ts",
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts"
    },
    "./Connector": {
      "bun": "./src/Connector.ts",
      "types": "./src/Connector.ts",
      "import": "./src/Connector.ts"
    }
  },
  "dependencies": {
    "@evmcrispr/sdk": "workspace:*"
  },
  "devDependencies": {
    "@evmcrispr/test-utils": "workspace:*",
    "is-ipfs": "^8.0.4"
  },
  "peerDependencies": {
    "viem": "^2.29.4"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "biome check src",
    "type-check": "tsc --noEmit",
    "build": "bun run ../../packages/sdk/scripts/build.ts",
    "test:unit": "bun test ./test/unit",
    "test:integration": "bun test ./test/integration"
  }
}

{
  "name": "@1hive/evmcrispr",
  "version": "0.10.4",
  "codename": "collective wisdom",
  "license": "GPL-3.0",
  "description": "A library that encapsulates actions in EVM scripts for DAOs",
  "author": "Blossom Labs",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox-viem": "^3.0.0",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@repo/prettier-config": "*",
    "@types/mocha": "^10.0.7",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/lodash.clonedeep": "^4.5.9",
    "chai": "^5.3.3",
    "msw": "^2.12.9",
    "node-fetch": "^3.3.2",
    "nyc": "^17.1.0",
    "ora": "^9.3.0",
    "is-ipfs": "^8.0.4",
    "hardhat": "^2.28.4",
    "typescript": "^5.9.3",
    "dotenv": "^17.2.4",
    "eslint": "^10.0.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@ensdomains/content-hash": "^3.0.0",
    "arcsecond": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jsymbol": "^0.3.5",
    "lodash.clonedeep": "^4.5.0"
  },
  "peerDependencies": {
    "viem": "^2.29.4"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "chain": "hardhat node",
    "create-fixtures": "hardhat run scripts/create-fixtures.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "hardhat test",
    "test:coverage": "nyc --reporter=lcov bun run test",
    "build": "rm -rf dist && bun build ./src/index.ts --outdir ./dist --format esm --sourcemap=linked --packages external && tsc -p tsconfig.build.json",
    "dev": "bun build ./src/index.ts --outdir ./dist --format esm --sourcemap=linked --packages external --watch"
  }
}
